{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "build": "npx tsc",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "db-migrate --env test reset -c 4 && db-migrate --env test up:all -c 4 && cross-env ENV=test jasmine-ts && db-migrate --env test down:all -c 4",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc src/**/* --write"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.8.4"
  }
